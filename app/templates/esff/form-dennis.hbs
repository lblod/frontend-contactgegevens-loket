<RdfForm
  @groupClass="au-o-grid__item"
  @form={{@model.form}}
  @graphs={{@model.graphs}}
  @sourceNode={{@model.sourceNode}}
  @formStore={{@model.formStore}}
  @show={{false}}
  @forceShowErrors={{false}}
  @useNewListingLayout={{true}}
/>
<AuButton
  @skin="primary"
  type="button"
  {{on "click" this.validateForm}}
>Validate</AuButton>